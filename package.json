  "scripts": {
    "dev": "ts-node nodemon server.ts",
    "clean": "rm -rf build",
    "build": "npm run build",
    "start": "npm run build && node build/index.js",
    "typeorm": "ts-node -O '{\"module\":\"commonjs\"}' node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm -- migration:generate",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert",
    "migration:show": "npm run typeorm -- migration:show",
    "migration:generate:custom": "npm run typeorm -- -d src/configs/database.ts migration:generate scripts/migrations/LikeCommentOnEventMigrations",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
